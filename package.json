{
  "name": "toolbox",
  "private": true,
  "scripts": {
    "precm": "git config hooks.prepare-commit-msg.disabled true",
    "postcm": "git config hooks.prepare-commit-msg.disabled false",
    "cm": "npx @rareyes/commitizen-config",
    "cmr": "npm run cm -- --retry",
    "lint": "lerna run lint",
    "test": "lerna run test"
  },
  "devDependencies": {
    "@rareyes/commitizen-config": "^1.0.3",
    "@rareyes/prettier-config": "^0.1.6",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lerna": "^3.21.0",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5"
  }
}
