{
  "name": "toolbox",
  "private": true,
  "scripts": {
    "precm": "git config hooks.prepare-commit-msg.disabled true",
    "postcm": "git config hooks.prepare-commit-msg.disabled false",
    "cm": "npx @rareyes/commitizen-config",
    "cmr": "npm run cm -- --retry",
    "bootstrap": "npx lerna bootstrap --hoist",
    "lint": "eslint .",
    "build": "lerna run build"
  },
  "devDependencies": {
    "@rareyes/commitizen-config": "^1.0.5",
    "@rareyes/eslint-config": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "husky": "^4.2.5",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  }
}
